{
  "expo": {
    "name": "Lena",
    "slug": "Lena",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEFDFB"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.francescopontrandolfo.lena",
      "buildNumber": "1",
      "infoPlist": {
        "NSContactsUsageDescription": "Lena uses your contacts to help you stay connected with the friends who matter most.",
        "NSPhotoLibraryUsageDescription": "Lena needs access to your photos to set profile pictures for your friends.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FEFDFB"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "6210eb8d-831c-4769-95fa-f7b9ccc266d9"
      }
    }
  }
}
